            <div class="col-md-4 col-sm-4 col-xs-4 col-xxs-4">
                <a class="add-custmoer-link" href="~/Customers/search"><i class="fas fa-search"></i> </a>
            </div>
            <div class="col-md-4 col-sm-4 col-xs-4 col-xxs-4 text-center">
                <h3 class="text-center user-login">
                    <i class="fas fa-user"></i>
                </h3>
                <p style="margin-bottom:0;" class=" text-dark">@Session["UserName"]</p>
                @Html.TextBox("UserName", @Session["UserName"], new { @style = "display:none" })
